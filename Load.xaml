<Window x:Class="ElektronikusEllenorzo.Load"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ElektronikusEllenorzo"
        mc:Ignorable="d"
        Title="Load" Height="272" Width="400" ResizeMode="CanMinimize">
    <Grid>
        <ListBox x:Name="saveFiles" d:ItemsSource="{d:SampleData ItemCount=5}" Height="192" VerticalAlignment="Top"/>

        <Button x:Name="load" Content="Load" HorizontalAlignment="Center" Margin="0,204,0,0" VerticalAlignment="Top" Click="load_Click"/>
        <Button x:Name="Vissza" Content="Vissza" HorizontalAlignment="Left" Margin="10,204,0,0" VerticalAlignment="Top" Click="Vissza_Click"/>

    </Grid>
</Window>
